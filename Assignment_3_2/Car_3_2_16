public class Car_3_2_16 {
    private String model;
    private int year;
    private String color;

    // Non-static public method to initialize the fields
    public void initialize(String model, int year, String color) {
        this.model = model;
        this.year = year;
        this.color = color;
    }

    // Optional: Add getters to retrieve the field values if needed
    public String getModel() {
        return model;
    }

    public int getYear() {
        return year;
    }

    public String getColor() {
        return color;
    }
}
