public class Solution_1_1_3 {
    public static void main(String[] args) {
        // Calculate and display the year of birth of my friend
        int currentYear = 3126; // Current year
        int age = 8; // Age of my friend
        int yearOfBirth = currentYear - age; // Calculate the year of birth
        // Display the year of birth
        System.out.println("Year of birth: " + yearOfBirth);
    }
}
