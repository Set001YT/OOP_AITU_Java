public class Solution_1_1_2 {
    public static void main(String[] args) {
        // Declare and initialize variables a and b
        int a = 1; // Assign any integer value to a
        int b = 10; // Assign a different integer value to b
        // Optional: Print the variables to verify their values
        System.out.println("a: " + a);
        System.out.println("b: " + b);
    }
}
